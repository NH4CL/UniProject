@{
    ViewBag.Title = "PurchaseHistoryDefination";
}

<script type = "text/javascript">
    var URLParams = new Array();
    var aParams = document.location.search.substr(1).split('?').toString();
    var aParam = aParams.split('=');
    URLParams[aParam[0]] = aParam[1];
    var id = URLParams["id"];

    $(document).ready(function () {
        $.ajax({
            type: "POST",
            url: "/Purchase/setHistoryId",
            data: { strId: id },
            datatype: "html",
            success: function (data) {
                window.location.href = "/Purchase/PurchaseHistoryDetail";
            },
            error: function () {
                alert('@LiveAzure.Resource.Stage.OrganizationController.errorAlert');
            }
        });
    });
</script>
