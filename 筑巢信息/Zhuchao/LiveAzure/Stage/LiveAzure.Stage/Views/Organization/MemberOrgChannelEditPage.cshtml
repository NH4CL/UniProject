@{
    Layout = null;
}
@model LiveAzure.Models.Member.MemberOrgChannel
<script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
@using (Ajax.BeginForm("MemberOrgChannelSave", "Organization", new AjaxOptions { HttpMethod = "POST", OnSuccess = "MemberOrgChannelAddSuccess" }))
{
    <table class="DIY_table">
        <tr>
            <th>
                @LiveAzure.Resource.Model.Member.MemberOrgChannel.RemoteUrlLong
            </th>
            <td>
                @Html.EditorFor(model=>model.RemoteUrl)
            </td>
            <th>
                @LiveAzure.Resource.Model.Member.MemberOrgChannel.Cstatus
            </th>
            <td>
                @Html.DropDownListFor(model=>model.Cstatus, (List<SelectListItem>)ViewBag.ChlCStatus)
            </td>
        </tr>
        <tr>
            <th>
                @LiveAzure.Resource.Model.Member.MemberOrgChannel.ConfigKey
            </th>
            <td>
                @Html.EditorFor(model=>model.ConfigKey)
            </td>
            <th>
                @LiveAzure.Resource.Model.Member.MemberOrgChannel.SecretKey
            </th>
            <td>
                @Html.EditorFor(model=>model.SecretKey)
            </td>
        </tr>
        <tr>
            <th>
                @LiveAzure.Resource.Model.Member.MemberOrgChannel.SessionKeyLong
            </th>
            <td>
                @Html.EditorFor(model=>model.SessionKey)
            </td>
            <th>
                @LiveAzure.Resource.Model.ModelBase.Remark
            </th>
            <td>
                @Html.EditorFor(model=>model.Remark)
            </td>
        </tr>
    </table>
    <div>
        <input type="submit" value="@LiveAzure.Resource.Stage.OrganizationController.submit" />
        <input type="reset" value="@LiveAzure.Resource.Stage.OrganizationController.reset" />
    </div>
}