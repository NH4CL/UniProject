    @Html.EditorFor(model => model.Contact)
    @Html.ValidationMessageFor(model => model.Contact)
    </div>
    
    <div>
    @LiveAzure.Resource.Model.Member.OrganizationBase.WorkPhone
    @Html.EditorFor(model => model.WorkPhone)
