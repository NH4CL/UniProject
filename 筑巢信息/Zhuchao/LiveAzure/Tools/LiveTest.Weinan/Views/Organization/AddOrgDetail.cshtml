@model LiveAzure.Models.Member.MemberOrganization
           
@{
    ViewBag.Title = "组织详情";
}

@using (Html.BeginForm("AddOrgDetail","Organization",FormMethod.Post))
{
    <div>
    @LiveAzure.Resource.Model.Member.OrganizationBase.Code
